{
	"name": "the-blind-hunt",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"build": "tsc",
		"bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
		"android": "yarn run build && yarn run force-android",
		"force-android": "yarn run bundle && react-native run-android",
		"clean-android": "cd android && ./gradlew clean && cd .."
	},
	"dependencies": {
		"axios": "^0.18.0",
		"firebase": "^4.11.0",
		"firebase-admin": "^5.10.0",
		"firebase-functions": "^0.8.1",
		"moment": "^2.21.0",
		"react": "16.2.0",
		"react-native": "0.53.3",
		"react-native-elements": "^0.19.0",
		"react-native-firebase": "^3.3.1",
		"react-native-google-signin": "^0.12.0",
		"react-native-keyboard-spacer": "^0.4.1",
		"react-native-loading-spinner-overlay": "^0.5.2",
		"react-native-maps": "^0.20.1",
		"react-native-redux-listener": "^0.3.0",
		"react-native-router-flux": "^4.0.0-beta.28",
		"react-native-sensors": "^2.4.2",
		"react-native-simple-compass": "^1.0.0",
		"react-native-svg": "^6.2.2",
		"react-native-vector-icons": "^4.5.0",
		"react-navigation": "^1.2.1",
		"react-redux": "^5.0.7",
		"redux": "^3.7.2",
		"redux-thunk": "^2.2.0"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/firebase": "^3.2.1",
		"@types/moment": "^2.13.0",
		"@types/react": "^16.0.40",
		"@types/react-native": "^0.52.17",
		"@types/react-native-elements": "^0.18.0",
		"@types/react-native-google-signin": "^0.12.1",
		"@types/react-native-loading-spinner-overlay": "^0.5.0",
		"@types/react-navigation": "^1.2.1",
		"@types/react-redux": "^5.0.15",
		"@types/redux": "^3.6.0",
		"@types/redux-thunk": "^2.1.0",
		"babel-jest": "22.4.1",
		"babel-preset-react-native": "4.0.0",
